{
  "name": "aforis",
  "version": "1.0.0",
  "description": "Web Game",
  "main": "webpack.config.js",
  "scripts": {
    "start": "tsc -p ./tsconfig.json --downlevelIteration\nwebpack -c ./webpack.config.js --mode production\nnode compiledTS/server/server",
    "server": "tsc server/server.ts\nnode compiledTS/server/server",
    "startTest": "tsc -p ./tsconfig.json --downlevelIteration\nbrowserify compiledTS/src/index.js -o dist/main.js -d\nnode compiledTS/server/server",
    "client": "tsc -p ./tsconfig.json --downlevelIteration\nbrowserify compiledTS/src/index.js -o dist/main.js -d",
    "filter": "tsc -p ./tsconfig.json server/gameSettings/filtersSet.ts/gameSettings\nnode server/gameSettings/filtersSet.js"
  },
  "author": ".zirolio.",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "browserify": "^17.0.0",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "phaser": "^3.60.0",
    "planck": "^1.0.0-alpha.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.6",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  }
}
